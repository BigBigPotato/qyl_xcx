<!--pages/login.wxml-->
<view>
  <view class='logo-container'>
    <image src='/statics/images/my_icon_qiuyoule@3x.png' class='logo'></image>
    <text>qiuyoule.com</text>
  </view>
  <view>
    <input placeholder='账号' class='input-style' bindblur='getAcount'></input>
    <input type='password' placeholder='密码' class='input-style' bindblur='getPwd'></input>
    <button class='login-btn' catchtap='toLogin'>登录</button>
  </view>
  <view class='other'>
    <navigator>注册</navigator>
  </view>
</view>
